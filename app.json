{
  "expo": {
    "name": "outBard",
    "slug": "outBard",
    "version": "1.0.0",
    "orientation": "potrait",
    "icon": "./assets/outbard-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#292230"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "userInterfaceStyle": "automatic",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "userInterfaceStyle": "automatic"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "scheme": "outBard",
    "plugins": [
      "expo-router"
    ]
  }
}
